{
  "schemaVersion": 1,
  "id": "genesisframework",
  "version": "${version}",
  "name": "Genesis Framework",
  "description": "A framework for creating custom age progression systems.",
  "authors": [
    "uhmbyee / Mariany Pinkston"
  ],
  "contact": {
    "homepage": "https://mariany.dev/",
    "sources": "https://github.com/marianyp/Genesis-Framework"
  },
  "license": "MPL-2.0",
  "icon": "assets/genesisframework/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.mariany.genesisframework.GenesisFramework"
    ],
    "client": [
      "dev.mariany.genesisframework.client.GFClient"
    ],
    "fabric-datagen": [
      "dev.mariany.genesisframework.GFDataGenerator"
    ]
  },
  "mixins": [
    "genesisframework.mixins.json"
  ],
  "accessWidener": "genesisframework.accesswidener",
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": ">=1.21.6",
    "java": ">=21",
